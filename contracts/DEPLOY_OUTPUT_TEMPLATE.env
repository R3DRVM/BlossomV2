# Blossom Execution Contracts - Post-Deployment Configuration Template
# Copy this file to your backend .env and fill in the values from deployment output

# Execution Router Address
# Copy from deployment output: "ExecutionRouter deployed at: 0x..."
EXECUTION_ROUTER_ADDRESS=

# Mock Swap Adapter Address
# Copy from deployment output: "MockSwapAdapter deployed at: 0x..."
MOCK_SWAP_ADAPTER_ADDRESS=

# ERC20 Pull Adapter Address
# Copy from deployment output: "ERC20PullAdapter deployed at: 0x..."
ERC20_PULL_ADAPTER_ADDRESS=

# Demo Tokens (for investor demo)
# Copy from deployment output
DEMO_REDACTED_ADDRESS=
DEMO_WETH_ADDRESS=
DEMO_SWAP_ROUTER_ADDRESS=

# Uniswap V3 Swap Adapter Address (uses DemoSwapRouter for deterministic demo)
# Copy from deployment output: "UniswapV3SwapAdapter deployed at: 0x..."
UNISWAP_ADAPTER_ADDRESS=

# Uniswap V3 SwapRouter02 Address (required if deploying Uniswap adapter)
# Copy from deployment output: "UNISWAP_V3_ROUTER_ADDRESS= 0x..."
UNISWAP_V3_ROUTER_ADDRESS=

# Ethereum Sepolia Testnet Chain ID (fixed)
ETH_TESTNET_CHAIN_ID=11155111

# Ethereum Sepolia RPC URL
# Use the same RPC URL you used for deployment (e.g., Infura, Alchemy)
ETH_TESTNET_RPC_URL=

# Sepolia Token Addresses (for swap execution)
REDACTED_ADDRESS_SEPOLIA=0x1c7D4B196Cb0C7B01d743Fbc6116a902379C7238
WETH_ADDRESS_SEPOLIA=0x7b79995e5f793A07Bc00c21412e50Ecae098E7f9

# Uniswap V3 SwapRouter02 on Sepolia
SEPOLIA_UNISWAP_V3_ROUTER=0x3bFA4769FB09eef5735E1C5B5C0C5B5C5B5C5B5C

# Optional: Execution Router Owner Address (if you need to manage adapters)
# This is the address that deployed the router (your deployer address)
# EXECUTION_ROUTER_OWNER_ADDRESS=

