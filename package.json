{
  "name": "blossom-ai-trading-copilot",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fresh": "rm -rf node_modules/.vite .vite 2>/dev/null; vite --force",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:agent": "cd agent && npm run dev:agent",
    "dev:all": "concurrently --kill-others-on-fail --names \"frontend,agent\" --prefix-colors \"cyan,green\" --prefix \"[{name}]\" \"npm run dev\" \"npm run dev:agent\"",
    "dev:demo": "npx concurrently --kill-others-on-fail --names \"frontend,backend\" --prefix-colors \"blue,green\" \"npm run dev\" \"cd agent && PORT=3001 npm run dev\"",
    "install:all": "npm install && cd agent && npm install",
    "postinstall": "cd agent && npm install",
    "stress-test": "npx tsx scripts/stress-test.ts",
    "stress-test:heavy": "STRESS_CONCURRENCY=10 STRESS_TOTAL_REQUESTS=100 npx tsx scripts/stress-test.ts",
    "stress:chat-only:prod": "npx tsx agent/scripts/live-stress-tester.ts --baseUrl=https://api.blossom.onl --mode=chat_only --count=20 --concurrency=4 --output=logs/stress-chat-only-prod.json",
    "stress:mixed:prod": "npx tsx agent/scripts/live-stress-tester.ts --baseUrl=https://api.blossom.onl --mode=mixed --count=20 --concurrency=4 --output=logs/stress-mixed-prod.json",
    "stress:tier1:deterministic": "npx tsx agent/scripts/live-stress-tester.ts --baseUrl=https://api.blossom.onl --mode=tier1 --allow_execute --count=40 --concurrency=2 --output=logs/stress-tier1-deterministic.json",
    "stress:tier1:relayed_required": "npx tsx agent/scripts/live-stress-tester.ts --baseUrl=https://api.blossom.onl --mode=tier1_relayed_required --allow_execute --count=40 --concurrency=2 --output=logs/stress-tier1-relayed-required.json",
    "stress:tier1:crosschain_required": "npx tsx agent/scripts/live-stress-tester.ts --baseUrl=https://api.blossom.onl --mode=tier1_crosschain_required --allow_execute --count=10 --concurrency=2 --output=logs/stress-tier1-crosschain-required.json",
    "mvp:prove:prod": "npx tsx agent/scripts/mvp-prove-prod.ts",
    "stress:tier2:realistic": "npx tsx agent/scripts/live-stress-tester.ts --baseUrl=https://api.blossom.onl --mode=tier2 --allow_execute --count=40 --concurrency=2 --output=logs/stress-tier2-realistic.json"
  },
  "dependencies": {
    "@jridgewell/gen-mapping": "^0.3.13",
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@sentry/react": "^8.12.0",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.90.20",
    "buffer": "^6.0.3",
    "lucide-react": "^0.556.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.10.1",
    "viem": "^2.43.3",
    "wagmi": "^2.19.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/node": "^22.19.11",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@vercel/node": "^5.5.27",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "esbuild": "^0.27.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
