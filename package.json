{
  "name": "blossom-ai-trading-copilot",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:agent": "cd agent && npm run dev:agent",
    "dev:all": "concurrently --kill-others-on-fail --names \"frontend,agent\" --prefix-colors \"cyan,green\" --prefix \"[{name}]\" \"npm run dev\" \"npm run dev:agent\"",
    "dev:demo": "npx concurrently --kill-others-on-fail --names \"frontend,backend\" --prefix-colors \"blue,green\" \"npm run dev\" \"cd agent && PORT=3001 npm run dev\"",
    "install:all": "npm install && cd agent && npm install"
  },
  "dependencies": {
    "@jridgewell/gen-mapping": "^0.3.13",
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.90.20",
    "lucide-react": "^0.556.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.10.1",
    "viem": "^2.43.3",
    "wagmi": "^2.19.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
