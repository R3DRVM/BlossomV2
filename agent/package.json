{
  "name": "blossom-agent",
  "version": "0.1.0",
  "description": "Blossom AI Trading Copilot - Backend Agent Service",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server/http.ts",
    "dev:agent": "tsx watch src/server/http.ts",
    "build": "tsc",
    "start": "node dist/src/server/http.js",
    "type-check": "tsc --noEmit",
    "e2e:mvp-full-flow": "tsx scripts/e2e-mvp-full-flow.ts",
    "prove:session-authority": "tsx scripts/prove-session-authority.ts",
    "prove:execution-kernel": "tsx scripts/prove-execution-kernel.ts",
    "prove:dflow-routing": "tsx scripts/prove-dflow-routing.ts",
    "prove:new-user-wow": "tsx scripts/prove-new-user-wow.ts",
    "prove:defi-execution:dry-run": "tsx scripts/prove-defi-execution-dry-run.ts",
    "prove:aave-defi:preflight": "tsx scripts/prove-aave-defi-preflight.ts",
    "prove:aave-defi:dry-run": "tsx scripts/prove-aave-defi-dry-run.ts",
    "prove:aave-defi:live-read": "tsx scripts/prove-aave-defi-live-read.ts",
    "prove:aave-adapter:deployed": "tsx scripts/prove-aave-adapter-deployed.ts",
    "prove:aave-defi:prereqs": "tsx scripts/prove-aave-defi-prereqs.ts",
    "prove:aave-defi:e2e-smoke": "tsx scripts/prove-aave-defi-e2e-smoke.ts",
    "prove:aave-defi:post-tx": "tsx scripts/prove-aave-defi-post-tx.ts",
    "prove:aave-defi:real": "tsx scripts/prove-aave-defi-real.ts",
    "prove:real": "tsx scripts/prove-real.ts",
    "prove:aave-defi:withdraw:dry-run": "tsx scripts/prove-aave-defi-withdraw-dry-run.ts",
    "stress:aave-positions": "tsx scripts/stress-test-aave-positions.ts",
    "stress:routing": "tsx scripts/stress-test-routing.ts",
    "sprint4:activate": "tsx scripts/sprint4-activate.ts",
    "prove:all": "npm run prove:execution-kernel && npm run prove:session-authority && npm run prove:dflow-routing && npm run prove:new-user-wow && npm run prove:defi-execution:dry-run && npm run prove:aave-defi:preflight && npm run prove:aave-defi:dry-run && npm run prove:aave-defi:live-read && npm run prove:aave-defi:withdraw:dry-run && npm run prove:aave-adapter:deployed || true && npm run prove:aave-defi:e2e-smoke || true && npm run prove:aave-defi:post-tx || true && STRESS_CONCURRENCY=100 npm run stress:aave-positions && NON_INTERACTIVE=true STRESS_CONCURRENCY=50 npm run stress:routing",
    "load-test": "tsx scripts/load-test-users.ts",
    "prove:telemetry:db": "tsx scripts/prove-telemetry-db.ts",
    "prove:telemetry:harness": "tsx scripts/prove-telemetry-harness.ts",
    "devnet:load": "tsx scripts/devnet-load-test.ts",
    "devnet:exec-batch": "tsx scripts/devnet-exec-batch.ts",
    "devnet:report": "tsx scripts/generate-devnet-report.ts",
    "prove:devnet:stats": "tsx scripts/prove-devnet-stats.ts stats",
    "prove:devnet:load:smoke": "tsx scripts/prove-devnet-stats.ts load-smoke",
    "prove:devnet:ui:stats": "tsx scripts/prove-devnet-stats.ts ui-stats",
    "prove:devnet:rpc:health": "tsx scripts/prove-devnet-stats.ts rpc-health",
    "prove:devnet:all": "tsx scripts/prove-devnet-stats.ts all",
    "devnet:campaign": "tsx scripts/devnet-campaign.ts",
    "prove:devnet:campaign:smoke": "tsx scripts/devnet-campaign.ts --stages=50:20:15"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@sentry/node": "^10.38.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/pg": "^8.16.0",
    "better-sqlite3": "^12.6.2",
    "bs58": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "openai": "^4.20.0",
    "pg": "^8.17.2",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.1",
    "viem": "^2.43.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
